{
  "name": "better-memory-cache",
  "version": "0.0.0-development",
  "description": "A memory cache written in Typescript with support for stale-while-revalidate and background refresh",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "demo": "tsx ./tests/demo.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "9.14.0",
    "globals": "^15.12.0",
    "mustache": "^4.2.0",
    "semantic-release": "^24.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vitest": "^2.1.5"
  },
  "author": "blex41",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blex41/better-memory-cache.git"
  },
  "bugs": {
    "url": "https://github.com/blex41/better-memory-cache/issues"
  },
  "homepage": "https://github.com/blex41/better-memory-cache#readme",
  "keywords": [
    "cache",
    "memory",
    "stale",
    "refresh",
    "typescript"
  ],
  "publishConfig": {
    "provenance": true
  }
}
